<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/3.0/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/3.0/modelMap http://cayenne.apache.org/schema/3.0/modelMap.xsd"
	 project-version="6">
	<property name="defaultLockType" value="optimistic"/>
	<property name="defaultPackage" value="db"/>
	<property name="defaultCatalog" value="mxerp"/>
	<db-entity name="accounts" catalog="mxerp">
		<db-attribute name="account_type" type="VARCHAR" length="100"/>
		<db-attribute name="annual_revenue" type="VARCHAR" length="100"/>
		<db-attribute name="assigned_user_id" type="CHAR" length="36"/>
		<db-attribute name="billing_address_city" type="VARCHAR" length="100"/>
		<db-attribute name="billing_address_country" type="VARCHAR" length="255"/>
		<db-attribute name="billing_address_postalcode" type="VARCHAR" length="20"/>
		<db-attribute name="billing_address_state" type="VARCHAR" length="100"/>
		<db-attribute name="billing_address_street" type="VARCHAR" length="150"/>
		<db-attribute name="campaign_id" type="CHAR" length="36"/>
		<db-attribute name="created_by" type="CHAR" length="36"/>
		<db-attribute name="date_entered" type="TIMESTAMP" length="19"/>
		<db-attribute name="date_modified" type="TIMESTAMP" length="19"/>
		<db-attribute name="deleted" type="BIT"/>
		<db-attribute name="description" type="LONGVARCHAR" length="65535"/>
		<db-attribute name="employees" type="VARCHAR" length="10"/>
		<db-attribute name="id" type="CHAR" isPrimaryKey="true" isMandatory="true" length="36"/>
		<db-attribute name="industry" type="VARCHAR" length="50"/>
		<db-attribute name="modified_user_id" type="CHAR" length="36"/>
		<db-attribute name="name" type="VARCHAR" length="150"/>
		<db-attribute name="ownership" type="VARCHAR" length="100"/>
		<db-attribute name="parent_id" type="CHAR" length="36"/>
		<db-attribute name="phone_alternate" type="VARCHAR" length="100"/>
		<db-attribute name="phone_fax" type="VARCHAR" length="100"/>
		<db-attribute name="phone_office" type="VARCHAR" length="100"/>
		<db-attribute name="rating" type="VARCHAR" length="100"/>
		<db-attribute name="shipping_address_city" type="VARCHAR" length="100"/>
		<db-attribute name="shipping_address_country" type="VARCHAR" length="255"/>
		<db-attribute name="shipping_address_postalcode" type="VARCHAR" length="20"/>
		<db-attribute name="shipping_address_state" type="VARCHAR" length="100"/>
		<db-attribute name="shipping_address_street" type="VARCHAR" length="150"/>
		<db-attribute name="sic_code" type="VARCHAR" length="10"/>
		<db-attribute name="ticker_symbol" type="VARCHAR" length="10"/>
		<db-attribute name="website" type="VARCHAR" length="255"/>
	</db-entity>
	<obj-entity name="Accounts" className="db.Accounts" dbEntityName="accounts">
		<obj-attribute name="accountType" type="java.lang.String" db-attribute-path="account_type"/>
		<obj-attribute name="annualRevenue" type="java.lang.String" db-attribute-path="annual_revenue"/>
		<obj-attribute name="assignedUserId" type="java.lang.String" db-attribute-path="assigned_user_id"/>
		<obj-attribute name="billingAddressCity" type="java.lang.String" db-attribute-path="billing_address_city"/>
		<obj-attribute name="billingAddressCountry" type="java.lang.String" db-attribute-path="billing_address_country"/>
		<obj-attribute name="billingAddressPostalcode" type="java.lang.String" db-attribute-path="billing_address_postalcode"/>
		<obj-attribute name="billingAddressState" type="java.lang.String" db-attribute-path="billing_address_state"/>
		<obj-attribute name="billingAddressStreet" type="java.lang.String" db-attribute-path="billing_address_street"/>
		<obj-attribute name="campaignId" type="java.lang.String" db-attribute-path="campaign_id"/>
		<obj-attribute name="createdBy" type="java.lang.String" db-attribute-path="created_by"/>
		<obj-attribute name="dateEntered" type="java.util.Date" db-attribute-path="date_entered"/>
		<obj-attribute name="dateModified" type="java.util.Date" lock="true" db-attribute-path="date_modified"/>
		<obj-attribute name="deleted" type="java.lang.Boolean" db-attribute-path="deleted"/>
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
		<obj-attribute name="employees" type="java.lang.String" db-attribute-path="employees"/>
		<obj-attribute name="industry" type="java.lang.String" db-attribute-path="industry"/>
		<obj-attribute name="modifiedUserId" type="java.lang.String" db-attribute-path="modified_user_id"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
		<obj-attribute name="ownership" type="java.lang.String" db-attribute-path="ownership"/>
		<obj-attribute name="parentId" type="java.lang.String" db-attribute-path="parent_id"/>
		<obj-attribute name="phoneAlternate" type="java.lang.String" db-attribute-path="phone_alternate"/>
		<obj-attribute name="phoneFax" type="java.lang.String" db-attribute-path="phone_fax"/>
		<obj-attribute name="phoneOffice" type="java.lang.String" db-attribute-path="phone_office"/>
		<obj-attribute name="rating" type="java.lang.String" db-attribute-path="rating"/>
		<obj-attribute name="shippingAddressCity" type="java.lang.String" db-attribute-path="shipping_address_city"/>
		<obj-attribute name="shippingAddressCountry" type="java.lang.String" db-attribute-path="shipping_address_country"/>
		<obj-attribute name="shippingAddressPostalcode" type="java.lang.String" db-attribute-path="shipping_address_postalcode"/>
		<obj-attribute name="shippingAddressState" type="java.lang.String" db-attribute-path="shipping_address_state"/>
		<obj-attribute name="shippingAddressStreet" type="java.lang.String" db-attribute-path="shipping_address_street"/>
		<obj-attribute name="sicCode" type="java.lang.String" db-attribute-path="sic_code"/>
		<obj-attribute name="tickerSymbol" type="java.lang.String" db-attribute-path="ticker_symbol"/>
		<obj-attribute name="website" type="java.lang.String" db-attribute-path="website"/>
	</obj-entity>
</data-map>
