<ccStyleExtension>

    <styleValue>
        <name>tabbedLineForegroundDefault</name>
        <value>#000000</value>
    </styleValue>    
    <styleValue>
        <name>tabbedLineForegroundSelected</name>
        <value>#000000</value>
    </styleValue>    
    <styleValue>
        <name>gridselectioncolor1</name>
        <value>#FFC00020</value>
    </styleValue>    
    <styleValue>
        <name>gridselectioncolor2</name>
        <value>#FFC00040</value>
    </styleValue>    
    <styleValue>
        <name>focusbgpaint</name>
        <value>border(1,1,100%-2,100%-2,#c0404060,1);border(2,2,100%-4,100%-4,#c0404040,1);border(3,3,100%-6,100%-6,#c0404020,1)</value>
    </styleValue>    
    <styleValue>
        <name>focusbgpaint_cc_flexgridpane</name>
        <value>border(0,0,100%,100%,#c0404060,1)</value>
    </styleValue>    

    <styleValue>
        <name>simpleStyleManagement:cc_buttontabbedline</name>
